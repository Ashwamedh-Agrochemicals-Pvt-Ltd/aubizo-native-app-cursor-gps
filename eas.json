{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "APP_VARIANT": "development",
        "APP_BRAND": "aubizo"
      },
      "environment": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "APP_VARIANT": "preview",
        "APP_BRAND": "aubizo"
      },
      "environment": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "environment": "production",
      "env": {
        "APP_VARIANT": "production",
        "APP_BRAND": "aubizo"
      }
    },
    "ashwamedh-development": {
      "extends": "development",
      "env": {
        "APP_VARIANT": "development",
        "APP_BRAND": "ashwamedh"
      }
    },
    "ashwamedh-preview": {
      "extends": "preview",
      "env": {
        "APP_VARIANT": "preview",
        "APP_BRAND": "ashwamedh"
      }
    },
    "ashwamedh-production": {
      "extends": "production",
      "env": {
        "APP_VARIANT": "production",
        "APP_BRAND": "ashwamedh"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./ServiceAccountAubizo.json",
        "track": "production"
      }
    },
    "ashwamedh-production": {
      "android": {
        "serviceAccountKeyPath": "./ServiceAccountAshwamedh.json",
        "track": "production"
      }
    }
  }
}